<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ShopTARgv23</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ShopTARgv23.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/assets/css/main.css" />
</head>
<body>
    <header>
        <section id="header">
            <div class="container">
                <h1 id="logo"><a asp-area="" asp-controller="Home" asp-action="Index">Strongly Typed</a></h1>
                <nav id="nav">
                    <ul>
                        <li><a asp-area="" asp-controller="Home" asp-action="Index" class="icon solid fa-home"><span>Home</span></a></li>
                        <li><a asp-area="" asp-controller="Home" asp-action="Privacy" class="icon solid fa-cog"><span>Privacy</span></a></li>
                        <li><a asp-area="" asp-controller="Spaceships" asp-action="Index" class="icon solid fa-retweet"><span>Spaceship</span></a></li>
                        <li><a asp-area="" asp-controller="RealEstate" asp-action="Index" class="icon solid fa-sitemap"><span>Real Estate</span></a></li>
                    </ul>
                </nav>
            </div>
        </section>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer id="footer">
        <div class="container">
            &copy; 2024 - ShopTARgv23 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
